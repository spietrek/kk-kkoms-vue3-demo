{
  "name": "vue-3-starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore src",
    "lintfix": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "build:css": "postcss styles/index.css -o app/styles/index.css --env production",
    "build:run": "yarn build:css && yarn build"
  },
  "dependencies": {
    "@heroicons/vue": "^1.0.6",
    "@tailwindcss/line-clamp": "^0.3.1",
    "daisyui": "^2.2.2",
    "vue": "^3.2.31",
    "vue-router": "^4.0.13",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "postcss": "^8.4.7",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-nested-ancestors": "^2.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.0.23",
    "vite": "^2.8.6"
  },
  "engines": {
    "node": ">=14"
  }
}
